{
  "name": "boilerplate-frontend-v2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "final-form": "^4.19.1",
    "formik": "^2.1.4",
    "helmet": "^3.21.1",
    "i18next": "^19.3.4",
    "i18next-browser-languagedetector": "^3.0.3",
    "i18next-xhr-backend": "^3.2.2",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.1",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-final-form": "^6.3.0",
    "react-ga": "^2.7.0",
    "react-i18next": "^11.3.4",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.4",
    "spected": "^0.7.1",
    "styled-components": "^4.4.1",
    "warning": "^4.0.3",
    "yup": "^0.29.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "CI=false react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint:check": "eslint src --color",
    "lint:format": "eslint src --fix",
    "stylelint:check": "stylelint './src/**/*.js'",
    "storybook": "start-storybook -p 6006 -c ./.storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -s public",
    "prettier": "prettier --write 'src/**/*.{js,css}'",
    "prettier:check": "prettier --list-different 'src/**/*.{js,css}'",
    "prettier-changefiles": "pretty-quick --staged",
    "dev": "concurrently -n watch-frontend,watch-storybook -c blue,green \"yarn start\" \"yarn storybook\"",
    "check-vulnerabilities": "retire --outputformat json --outputpath artifacts/result-vulnerabilities.json",
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "check-bundle-size": "yarn build && size-limit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier-changefiles"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/locales/**",
      "!src/utils/test/**"
    ],
    "coverageReporters": [
      "html"
    ]
  },
  "devDependencies": {
    "@size-limit/preset-app": "4.4.4",
    "@size-limit/webpack": "4.4.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.3",
    "@storybook/addons": "^5.3.10",
    "@storybook/react": "^5.1.10",
    "@storybook/storybook-deployer": "^2.8.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "babel-eslint": "^10.0.3",
    "customize-cra": "^0.9.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.3",
    "jest-html-reporters": "^1.2.0",
    "jest-in-case": "^1.0.2",
    "jest-localstorage-mock": "^2.4.0",
    "jest-mock-console": "^1.0.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^2.1.5",
    "retire": "^2.0.3",
    "size-limit": "4.4.4",
    "source-map-explorer": "^2.2.2",
    "storybook-react-router": "^1.0.8",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "testing-library": "^0.0.2",
    "typescript": "^3.8.3",
    "yargs": "^15.0.2"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.18"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn"
  }
}
